frame (pack=true width=320 button="bt_cancel" location="" title="Enviando" resize=false) {
	head {
		label (id="lb_title" text="Efetuando o Login..." align="center")
		progress (id="pb_title")
		button (id="bt_cancel" text="Cancelar")
	}
	body {
		border (space=10) {
			north {
				"lb_title"
			}
			center {
				"pb_title"
			}
			south {
				flow {
					"bt_cancel"
				}
			}
		}
	}
}